<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapepr 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xxddongxx.calendar.mapper.AccountMapper">
    <select id="findByEmail" parameterType="String" resultType="Account">
        /* findByEmail */
        SELECT
            *
        FROM user
        WHERE
            email = #{value}
    </select>
</mapper>